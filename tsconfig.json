{
    "compilerOptions": {
        "allowJs": true,
        "alwaysStrict": true,
        "baseUrl": ".",
        "esModuleInterop": false,
        "lib": ["es2021", "dom"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmitHelpers": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": false,
        "outDir": "dist",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        },
        "resolveJsonModule": true,
        "sourceMap": false,
        "target": "esnext"
    },
    "typesRoots": [
        "node_modules/@types"
    ],
    "include": [
        "lib/**/*",
        "index.ts"
    ],
    "exclude": [
        "example/**/*",
        "tester/**/*",
        "Attic/**/*"
    ],
    "typedocOptions" : {
        "excludeExternals": true,
        "excludePrivate" : true,
        "excludeProtected" : false,
        "out" : "docs",
        "theme": "default",
        "includeVersion": true,
        "categorizeByGroup": true,
        "gitRemote": "origin",
        "hideGenerator": true,
        "sort" : ["static-first","visibility"],
        "name" : "Testing the Implementation of the RDF Canonicalization algorithm URDNA2015"
    }
}
